{
    "manifest_version": 3,
    "name": "Pokemon TCG Lookup",
    "description": "Quickly lookup Pokemon cards and read what they do in a small window.",
    "version": "1.0",
    "options_ui": {
        "page": "options.html"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "unlimitedStorage"
    ]
}
{
    "compilerOptions": {
        "strict": true,
        "target": "ES2022",
        "moduleResolution": "bundler",
        "module": "ES2022",
        "esModuleInterop": true,
        "sourceMap": false,
        "rootDir": "src",
        "outDir": "dist/js",
        "noEmitOnError": true,
        "jsx": "react",
        "typeRoots": [
            "node_modules/@types",
        ],
        "baseUrl": "src",
        "paths": {
            "@/*": [
                "./*"
            ],
            "@assets/*": [
                "./assets/*"
            ],
            "@images/*": [
                "./assets/images/*"
            ],
            "@components/*": [
                "./components/*",
                "./components/CardDisplayFrame/*",
                "./components/CardDisplayFrame/CardDisplay/*"
            ],
            "@models/*": [
                "./models/*"
            ],
            "@services/*": [
                "./services/*"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "@styles/*": [
                "./assets/styles/*"
            ]
        }
    },
    "include": [
        "src"
    ],
}